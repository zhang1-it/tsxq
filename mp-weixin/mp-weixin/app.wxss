  right: 0;
  bottom: 0;
  height: 100%;
  box-sizing: border-box;
}
.gui-transition-all {
  transition: all 0.2s ease-in 0s;
}
@keyframes gui-fade-in {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
.gui-fade-in {
  animation: gui-fade-in 200ms ease-in forwards;
}
@keyframes gui-fade-out {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
}
}
.gui-fade-out {
  animation: gui-fade-out 200ms ease-out forwards;
}
@keyframes gui-top-in {
0% {
    transform: translateY(-1000px);
}
100% {
    transform: translateY(0px);
}
}
.gui-top-in {
  animation: gui-top-in 200ms linear forwards;
}
@keyframes gui-top-out {
0% {
    transform: translateY(0px);
}
100% {
    transform: translateY(-1000px);
}
}
.gui-top-out {
  animation: gui-top-out 200ms linear forwards;
}
@keyframes gui-bottom-in {
0% {
    transform: translateY(600px);
}
100% {
    transform: translateY(0px);
}
}
.gui-bottom-in {
  animation: gui-bottom-in 200ms linear forwards;
}
@keyframes gui-bottom-out {
0% {
    transform: translateY(0px);
}
100% {
    transform: translateY(600px);
}
}
.gui-bottom-out {
  animation: gui-bottom-out 200ms linear forwards;
}
@keyframes gui-left-in {
0% {
    transform: translateX(-600px);
}
100% {
    transform: translateX(0px);
}
}
.gui-left-in {
  animation: gui-left-in 200ms linear forwards;
}
@keyframes gui-left-out {
0% {
    transform: translateX(0px);
}
100% {
    transform: translateX(-600px);
}
}
.gui-left-out {
  animation: gui-left-out 200ms linear forwards;
}
@keyframes gui-right-in {
0% {
    transform: translateX(600px);
}
100% {
    transform: translateX(0px);
}
}
.gui-right-in {
  animation: gui-right-in 200ms linear forwards;
}
@keyframes gui-right-out {
0% {
    transform: translateX(0px);
}
100% {
    transform: translateX(600px);
}
}
.gui-right-out {
  animation: gui-right-out 200ms linear forwards;
}
@keyframes gui-scale-in {
0% {
    transform: scale(0.3, 0.3);
}
100% {
    transform: scale(1, 1);
}
}
.gui-scale-in {
  animation: gui-scale-in 200ms linear forwards;
}
@keyframes gui-scale-out {
0% {
    transform: scale(1, 1);
}
100% {
    transform: scale(0.3, 0.3);
}
}
.gui-scale-out {
  animation: gui-scale-out 200ms linear forwards;
}
@keyframes gui-rotate360 {
0% {
    transform: rotate(0deg);
}
50% {
    transform: rotate(180deg);
}
100% {
    transform: rotate(360deg);
}
}
.gui-rotate360 {
  animation: gui-rotate360 1200ms infinite linear;
}

